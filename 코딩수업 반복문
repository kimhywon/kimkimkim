#include <stdio.h>

int main(){
int score;

printf("점수를 하나 입력하세요\n");
scanf("%d", &score);

  if(score>=90)
    { printf("A입니다.\n");}

  else if (score>=80)
  {
    printf("B\n");
  }
  else if (score>=70)
  {
    printf("C\n");
  }
  else if (score>=60)
  {
    printf("D\n");
  }
  else if (score>=0)
  {
    printf("f\n");
  }
  
return 0;
}

switch(score) 정수밖에 안됌!!!!!!!!!!!!!

