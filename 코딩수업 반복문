#include <stdio.h>

int main(){
  int count;

  count =1;

  while (count <= 3)
  {
      printf("count = %d\n", count);
      count=count+1;  \*이거 안 넣으면 무한 반복*\
		  무조건 프린트 아래에 넣어야함. 
 }

  printf("###프로그램종료\n");
  return 0;
}

