#include <stdio.h>

int main(){
  int count;
  int sum = 0;

  count = 1; 

  while (count <= 3)
  {
      sum = sum + count;
      printf("count = %d\tsum = %d\n", count, sum);
      count=count + 1;
  }

  printf("###합은: %d\n", count);
  return 0;
}

